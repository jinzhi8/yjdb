<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
		"http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="yuyuehmForm" type="com.kizsoft.oa.yuyue.config.YuyuehmConfigForm"/>
	</form-beans>
	<action-mappings>
		<action name="yuyuehmForm" path="/YuyuehmConfigAction" scope="request" type="com.kizsoft.oa.yuyue.config.YuyuehmConfigAction">
			<forward name="success" redirect="true" contextRelative="false" path="/yuyuehmConfig.jsp"/>

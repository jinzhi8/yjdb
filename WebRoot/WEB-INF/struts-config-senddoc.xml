<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
		"http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<action-mappings>
		<action path="/saveFileTypeAction" type="com.kizsoft.oa.senddoc.actions.SaveFileTypeAction">
			<forward contextRelative="false" path="/fileType.jsp" name="success"/>
		</action>

		<action path="/editFileTypeAction" type="com.kizsoft.oa.senddoc.actions.EditFileTypeAction">
			<forward contextRelative="false" path="/fileType.jsp" name="success"/>
		</action>

		<action path="/saveIssueCodeAction" type="com.kizsoft.oa.senddoc.actions.SaveIssueCodeAction">
			<forward contextRelative="false" path="/issueCode.jsp" name="success"/>
		</action>

		<action path="/editIssueCodeAction" type="com.kizsoft.oa.senddoc.actions.EditIssueCodeAction">
			<forward contextRelative="false" path="/issueCode.jsp" name="success"/>
		</action>

		<action path="/doGetAction" type="com.kizsoft.oa.senddoc.actions.DoGetAction">
			<forward name="success" contextRelative="false" path="/../getdoc/index.jsp" redirect="true"/>
		</action>

	</action-mappings>
</struts-config>

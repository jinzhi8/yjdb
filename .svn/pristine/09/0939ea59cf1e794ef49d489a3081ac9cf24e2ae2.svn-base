*{
	font-size: 14px;
	color: #354052;
	padding: 0px;
	margin: 0px;
	list-style: none;
	border: none;
	outline: none;
	font-family: 'Microsoft Yahei';
}
body,html{
	height: 100%;
	margin: 0px;
	padding: 0px;
	overflow: hidden;
}
.pyga_zdpyci_rightmian_publicwidth{
	width: 96%;
	margin: 0 auto;
	position: relative;
}
.pyga_zdpyci_header{
	height: 70px;
	line-height: 70px;
	position: absolute;
	width: 100%;
	filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#2da1f7,endcolorstr=#1a92ec,gradientType=0);
	-ms-filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#2da1f7,endcolorstr=#1a92ec,gradientType=0);
	background: #2da1f7;
	background: -moz-linear-gradient(top, #2da1f7, rgba(0, 0, 255, 0.5));
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#2da1f7), to(#1a92ec));
	background: -o-linear-gradient(top, #2da1f7, rgba(0, 0, 255, 0.5));
}
.pyga_zdpyci_logo{
	font-size: 19px;
	text-indent: 53px;
	color: white;
	background: url(../images/pyga_zdpyci_logo.png) no-repeat left center;
	margin-left: 12px;
	float: left;
}
.pyga_zdpyci_users_name{
	float: right;
	height: 70px;
	line-height: 70px;
}
.pyga_zdpyci_users_name a{
	color: white;
	text-decoration: none;
	font-size: 16px;
	cursor: pointer;
	height: 70px;
	line-height: 70px;
}
.pyga_zdpyci_users_name span{
	margin: 0px 10px;
	color: white;
	text-decoration: none;
	font-size: 16px;
	height: 70px;
	line-height: 70px;
}
.pyga_zdpyci_users_name span span{
	padding-left: 39px;
	margin: 0px;
}
.usericons{
	width: 36px;
	height: 70px;
	display: inline-block;
	background: url(../images/usericons.png) no-repeat center center;
	position: absolute;
}
.pyga_zdpyci_usersexit{
	width: 32px;
	height: 32px;
	display: table;
	background: url(../images/userexit.png) no-repeat center center;
	margin-top: 20px;
	float: right;
	margin-right: 50px;
	cursor: pointer;
}
.pyga_zdpyci_body{
	padding-top: 70px;
	min-height: 100%;
	box-sizing: border-box;
}
.pyga_zdpyci_leftnav{
	position: absolute;
	left: 0px;
	width: 256px;
	top: 70px;
	bottom: 0px;
	background-color: #1b2431;
	overflow: auto !important;
}
.menu_head{
	height: 47px;
	line-height: 47px;
	text-indent: 23px;
	font-size: 15px;
	color: white;
	cursor: pointer;
	position: relative;
	margin: 0px;
	background: url(../images/pro_left.png) no-repeat 89% 50% transparent;
}
.menu_head:hover{
	background-color: #415777;
}
.menu_head a{
	color: white;
	text-decoration: none;
}
.menu_list .current{
	background: url(../images/pro_down.png) no-repeat 89% 50% #313d4f;
	color: #1B93EC;
}
.currentss{
	color: #DA932C !important;
}
.menu_body{
	line-height: 38px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	display: none;
}
.menu_body a{
	display: block;
	height: 38px;
	line-height: 38px;
	text-indent: 30px;
	color: #f6f5f5;
	background: #162031;
	text-decoration: none;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.menu_body a:hover{
	text-decoration: none;
	color: #74a63e;
}
.pyga_zdpyci_rightmian{
	position: absolute;
	left: 256px;
	right: 0px;
	top: 70px;
	bottom: 0px;
	background-color: #eff3f6;
	overflow: auto !important;
}
.pyga_zdpyci_rightmianheader{
	height: 70px;
	line-height: 70px;
	width: 100%;
	background-color: white;
}
.pyga_zdpyci_rightmian_four_modulesboxs{
	position: relative;
	width: 100%;
	height: 150px;
	margin: 22px 0px;
}
.pyga_zdpyci_rightmian_four_modules{
	position: absolute;
	left: -10px;
	right: -10px;
}
.pyga_zdpyci_rightmian_four_modules li{
	width: 25%;
	float: left;
}
.pyga_zdpyci_rightmian_four_modules li div{
	margin: 0px 10px;
	height: 150px;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	color: white;
	padding: 0px 15px;
}
.pyga_zdpyci_rightmian_four_modules li div:hover{
	cursor: pointer;
	background-color: #1564F3 !important;
}
.pyga_zdpyci_rightmian_four_modules li:nth-child(1) div{
	background-color: #1b93ec
}
.pyga_zdpyci_rightmian_four_modules li:nth-child(2) div{
	background-color: #d65b4a
}
.pyga_zdpyci_rightmian_four_modules li:nth-child(3) div{
	background-color: #66b92e
}
.pyga_zdpyci_rightmian_four_modules li:nth-child(4) div{
	background-color: #da932c
}
.pyga_zdpyci_rightmian_four_modules li div ul{
	width: 100%;
	display: table;
	padding: 15px 0px;
	border-bottom: 1px solid rgba(255, 255, 255, 0.24);
}
.pyga_zdpyci_rightmian_four_modules li div ul li{
	width: 100%;
	clear: both;
	color: white;
	font-size: 29px;
	line-height: 40px;
}
.pyga_zdpyci_rightmian_four_modules li div span{
	display: block;
	height: 38px;
	line-height: 38px;
	color: white;
	font-size: 16px;
}
.pyga_zdpyci_rightmian_echartdivboxs{
	position: relative;
	width: 100%;
	margin: 22px 0px;
}
.pyga_zdpyci_rightmian_echartdiv{
	width: 100%;
}
.pyga_zdpyci_rightmian_echartleftdivs{
	width: 442px;
	height: 460px;
}
.pyga_zdpyci_rightmian_echartleftdiv{
	width: 422px;
	height: 100%;
	background-color: white;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border: 1px solid #EAEAEA;
}
.pyga_zdpyci_rightmian_echartrightdiv{
	width: 100%;
	height: 460px;
	background-color: white;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border: 1px solid #EAEAEA;
}
.public_table_div{
	background-color: white;
	margin: 22px 0px;
	border: 1px solid #EAEAEA;
	border-bottom: none;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
}
.public_table_title{
	font-size: 18px;
	padding: 20px 0px 18px 18px;
}
.public_table{
	border-collapse: collapse;
	width: 100%;
	table-layout: fixed;
}
.public_table tr th{
	height: 40px;
	line-height: 40px;
	text-align: left;
	background-color: #f5f8fa;
	border-top: 1px solid #e7e9ed;
	border-bottom: 1px solid #e7e9ed;
	padding: 0px 20px;
	font-size: 16px;
	font-weight: normal;
	color: #7f8fa4;
}
.public_table tr th:nth-child(4){
	width: 130px;
}
.public_table tr td{
	height: 70px;
	line-height: 70px;
	text-align: left;
	border-top: 1px solid #e7e9ed;
	border-bottom: 1px solid #e7e9ed;
	padding: 0px 20px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.public_table tr td span:nth-child(1){
	width: 100%;
	display: table;
	line-height: 21px;
	font-size: 15px;
	color: #333c48;
}
.public_table tr td span:nth-child(2){
	width: 100%;
	display: table;
	line-height: 21px;
	font-size: 15px;
	color: #7f8fa4;
}
.public_table tr td input[type="button"]{
	color: white;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border: 1px solid #289b38;
	padding: 8px 31px;
	font-size: 15px;
	filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#39b44a,endcolorstr=#34aa44,gradientType=0);
	-ms-filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=#39b44a,endcolorstr=#34aa44,gradientType=0);
	background: #39b44a;
	background: -moz-linear-gradient(top, #39b44a, rgba(0, 0, 255, 0.5));
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#39b44a), to(#34aa44));
	background: -o-linear-gradient(top, #39b44a, rgba(0, 0, 255, 0.5));
	cursor: pointer;
}
.public_table tr:hover td,.public_table tr:hover td span{
	color: #1B93EC;
	cursor: pointer;
}
.public_table td input[type="button"]:hover{
	background: #1B93EC;
	border: 1px solid #1D80CA;
}
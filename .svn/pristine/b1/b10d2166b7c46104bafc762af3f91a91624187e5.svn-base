$empColor:#3296fa;
$backgroundColor:#f5f5f5;
$empColor:#233253;
$mainBlue:#3296FA;
$otheRed:#F25643;
$otheOrange:#FF943E;
$otheBlue:#576A95;
$otheGreen:#15BC83;
$otheYellow:#FEE151;

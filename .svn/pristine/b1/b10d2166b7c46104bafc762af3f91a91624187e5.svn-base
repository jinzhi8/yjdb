$wrapperHeight:95vh;
@font-face {
  font-family: 'iconfont';
  src: url('../font/iconfont.eot?t=1512436220877'); /* IE9*/
  src: url('../font/iconfont.eot?t=1512436220877#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../font/iconfont.ttf?t=1512436220877') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../font/iconfont.svg?t=1512436220877#iconfont') format('svg'); /* iOS 4.1- */
}
@mixin iconfont{
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-weight: 400;
}

@mixin ellipsis{
  display: inline-block;
  overflow: hidden;
  vertical-align: middle;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}
@mixin ellipBox {
  overflow:hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}

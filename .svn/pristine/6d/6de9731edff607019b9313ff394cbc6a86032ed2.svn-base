.dtree {
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #666;
	white-space: nowrap;
}
.dtree img {
	border: 0px;
	vertical-align: middle;
}
.dtreggge a {
	//color: #333;
	text-decoration: none;
}
.dtree a.node, .dtree a.nodeSel {
	white-space: nowrap;
	padding: 1px 2px 1px 2px;
}
.dtree a.nodddde:hover, .dtrddee a.nodeSel:hover {
	//color: #333;
	//text-decoration: underline;
}
.dTreeNode a.nodeSel {
	background:url(treeimg/select.gif) no-repeat;
	width:192px;
	border:0px;
	text-decoration: none;
	color:#fff;
}
.dtree .clip {
	overflow: hidden;
}

.topdTreeNode{
background:url(treeimg/top.gif);
height:27px;
width:192px;
}
.topdTreeNode a{
margin-left:15px;
color:#fff;
font-weight:bold;
font-size: 14px;
line-height:27px;
}


package com.kizsoft.commons.mxworkflow.bean;

public class Point {
	public static final long PX=40;
	public static final long PY=40;
	private long x;
	private long y;
	
	public Point(){
		
	}
	public Point(long x,long y){
		this.x=x;
		this.y=y;
	} 
	
	public long getX() {
		return x;
	}
	public void setX(long x) {
		this.x = x;
	}
	public long getY() {
		return y;
	}
	public void setY(long y) {
		this.y = y;
	}
	
	public long getPX() {
		return x+PX;
	}

	public long getPY() {
		return y+PY;
	}
}

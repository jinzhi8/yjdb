a.viewbutton22222 {
    background: transparent url('../images/viewbutton_right.gif') no-repeat scroll top right;
    color: #444;
    display: block;
    float: left;
    font: normal 12px arial, sans-serif;
    height: 24px;
    margin-right: 6px;
    padding-right: 18px; /* sliding doors padding */
    text-decoration: none;
}

a.viewbutton22222 span {
    background: transparent url('../images/viewbutton_left.gif') no-repeat;
    display: block;
    line-height: 14px;
    padding: 5px 0 5px 18px;
}
a.viewbutton22222:active {
    background-position: bottom right;
    color: #000;
    outline: none; /* hide dotted outline in Firefox */
}

a.viewbutton22222:active span {
    background-position: bottom left;
    padding: 6px 0 4px 18px; /* push text down 1px */
}



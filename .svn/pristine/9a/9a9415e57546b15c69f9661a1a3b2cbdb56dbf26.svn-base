/****************** 表单样式 ********************/
/*－－TD样式－－*/
div,table,td {
	word-wrap:break-word;word-break:normal;word-break:break-all;
}
table {
	border-collapse:collapse;
	table-layout: automatic;
	word-wrap:break-word;
	word-break:normal;
	word-break:break-all;
}
td {
	font-family: "宋体", Arial, Courier New;
	font-size: 12px;
	color: #333333;
	text-decoration: none
}

td_1 {
	font-size: 14px;
	line-height: 180%;
	letter-spacing: 0.1em
}

A:VISITED {
	font-family: "宋体";
	font-size: 9pt;
	text-decoration: none
}

A:HOVER {
	color: #0066ff;
	text-decoration: none;
}

A {
	font-family: "宋体";
	font-size: 9pt;
	color: #000000;
	text-decoration: none
}

p {
	margin-top: 1;
	margin-bottom: 1;
	line-height: 1.5;
	letter-spacing: 1.0;
}


.sl{
  width:700px;
  display: inline-block;
  overflow: hidden;
  vertical-align: middle;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}
.tip {
    display: inline-block;
    vertical-align: middle;
    border-radius: 2px;
    width: 30px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    /*font-size: 1.2rem;*/
    background-color: #999;
    margin-right: 5px;
}
.tip em {
    font-style:normal;
    color: #fff;
    position: relative;
    animation: myfirst .5s infinite;
}
.tip.new {
    background-color: transparent;
}
.tip.new em {
    color: #15BC83;
}
@keyframes myfirst {
    from {
        top: -2px;
    }
    to {
        top: 0;
    }
}
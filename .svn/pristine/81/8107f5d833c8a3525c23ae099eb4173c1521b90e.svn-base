body {
	color: #333333;
	margin: 0px;
	padding: 0px;
	font-size: 14px;
	background-attachment: fixed;
}

.description {
	font-size: 12px;
	color: #336699;
}

.border-index {
	border: 1px solid #B3A57B;
}

.smallcol {
	width: 160px;
	height: 100px;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-top-color: #FFFFFF;
	border-right-color: #FFFFFF;
	border-bottom-color: #FFFFFF;
	border-left-color: #FFFFFF;
}

form {
	MARGIN: 0px
}

.table_display_3 {
	height: 25px;
	background-color: #eeeeee;
}

a {
	color: #005FA9;
	text-decoration: underline;
}

a:link {
	color: #005FA9;
	text-decoration: underline;
}

a:visited {
	color: #005FA9;
	text-decoration: underline;
}

a:hover {
	color: #005FA9;
	text-decoration: underline;
}

span.label-error {
	color: red;
}

input.field-error {
	color: white;
	background-color: red;
}

table.input-error td.message {
	color: red;
	font-size: 14px;
}

.message {
	color: red;
	font-size: 14px;
}

table.header {
	width: 100%;
}

table.content {
	width: 100%;
	height: 240px;
	background-color: #FFFAE6;
}

table.footer {
	width: 100%;
}

table.table-view {
	font-size: 14px;
	background-color: #a5cdf0;
}

th.table-header {
	font-size: 14px;
	height: 25px;
	background-color: #d8eafb;
}

th.table-header a {
	color: black;
	text-decoration: none;
	font-size: 10pt;
	font-weight: bold;
}

th.table-header a:hover {
	text-decoration: underline;
}

tr.table-footer {
	background-color: white;
}

tr.odd {
	height: 25px;
	background-color: #eef7ff;
}

tr.even {
	height: 25px;
	background-color: white;
}

a.btn {
	color: #000000;
	padding-top: 4px;
	padding-left: 19px;
	padding-right: 19px;
	padding-bottom: 5px;
	font-size: 12px;
	background-repeat: no-repeat;
	background-position: center center;
	background-image: url("../images/bg_button.gif");
	width: 64px;
	height: 23px;
	border: 0;
	cursor: hand;
	letter-spacing: 1px;
	text-align: center;
	text-decoration: none;
	vertical-align: text-bottom;
}
